{
    "name": "WSL - Sudo - Pip",
    "cmd": "wsl sudo pip",
    "set": [
        {
            "args": [
                "config",
                "set",
                "global.proxy"
            ]
        }
    ],
    "unset": [
        {
            "args": [
                "config",
                "unset",
                "global.proxy"
            ]
        }
    ]
}
